{
  "name": "@enactprotocol/mcp-server",
  "version": "2.0.2",
  "description": "MCP protocol server for Enact tool integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "enact-mcp": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "dev": "bun run src/index.ts",
    "dev:http": "bun run src/index.ts --http"
  },
  "dependencies": {
    "@enactprotocol/execution": "workspace:*",
    "@enactprotocol/shared": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2"
  },
  "keywords": ["mcp", "model-context-protocol", "ai", "tools", "streamable-http"],
  "author": "Enact Protocol",
  "license": "Apache-2.0"
}
