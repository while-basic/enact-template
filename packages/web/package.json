{
  "name": "@enactprotocol/web",
  "version": "2.0.2",
  "description": "Enact Registry Browser & Auth Portal",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite && vite",
    "build": "tsc && vite build && bun run scripts/generate-blog-meta.ts",
    "build:vite": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.9",
    "lucide-react": "^0.307.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.21.1",
    "remark-gfm": "^4.0.1",
    "shiki": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "license": "Apache-2.0"
}
